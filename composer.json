{
    "name": "spryker-eco/showcase",
    "description": "Showcase for 3rd party vendor namespace'd bundles",
    "require": {
        "spryker/kernel": "^3.0.0"
    },
    "minimum-stability": "dev",
    "license": "proprietary",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\Zed\\Showcase\\": "src/SprykerEco/Zed/Showcase/",
            "SprykerEco\\Yves\\Showcase\\": "src/SprykerEco/Yves/Showcase/",
            "SprykerEco\\Client\\Showcase\\": "src/SprykerEco/Client/Showcase/",
            "SprykerEco\\Service\\Showcase\\": "src/SprykerEco/Service/Showcase/",
            "SprykerEco\\Shared\\Showcase\\": "src/SprykerEco/Shared/Showcase/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://code.spryker.com/repo/private"
        }
    ]
}
